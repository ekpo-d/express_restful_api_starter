  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ekpo-d/progress/issues"
  },
  "homepage": "https://github.com/ekpo-d/progress#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "cors": "2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1"
  },
