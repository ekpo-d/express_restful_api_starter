    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-watch": "2.1.11",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "1.8.0",
    "should": "^11.1.1",
    "supertest": "^2.0.1"
  }
}
